      <Label layoutX="33.0" layoutY="126.0" text="Bank Account Routing Number" />
      <Label layoutX="33.0" layoutY="99.0" text="Bank Account Number" />
      <Button fx:id="exitBtn" layoutX="515.0" layoutY="24.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
