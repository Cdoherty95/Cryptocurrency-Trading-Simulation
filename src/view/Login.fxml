<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Login">
   <children>
      <AnchorPane prefHeight="276.0" prefWidth="275.0">
         <children>
            <TextField fx:id="Username" layoutX="63.0" layoutY="64.0" promptText="Username" />
            <Button fx:id="loginBTN" layoutX="63.0" layoutY="182.0" mnemonicParsing="false" onAction="#loginbtnClicked" text="Login" />
            <PasswordField fx:id="Password" layoutX="63.0" layoutY="100.0" promptText="Password" />
            <Button fx:id="exitBTN" layoutX="119.0" layoutY="220.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
            <Label fx:id="Welcome" layoutX="43.0" layoutY="23.0" text="Welcome To Cryptocurrency Trader" />
            <Label fx:id="errorLable" layoutX="19.0" layoutY="137.0" prefHeight="18.0" prefWidth="238.0" />
            <Button fx:id="registerbtn" layoutX="165.0" layoutY="182.0" mnemonicParsing="false" onAction="#register" text="Register" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
