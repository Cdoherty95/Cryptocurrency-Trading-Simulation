<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminMenuController">
   <children>
      <VBox fx:id="border" layoutX="14.0" layoutY="93.0" prefHeight="164.0" prefWidth="158.0">
         <children>
            <Button fx:id="viewUsers" mnemonicParsing="false" onAction="#viewUsers" text="View All Users" />
            <Button fx:id="viewTransactionHistory" mnemonicParsing="false" onAction="#viewTransactionHistory" text="View All Transactions" />
            <Button fx:id="userMenu" mnemonicParsing="false" onAction="#backToUserMenu" text="Back To User Menu" />
         </children>
      </VBox>
      <Label fx:id="welcomeLabel" alignment="CENTER" layoutX="32.0" layoutY="40.0" prefHeight="51.0" prefWidth="235.0" text="Welcome">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="exitBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
      <Button fx:id="logoutBtn" layoutX="234.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout" text="Log Out" />
   </children>
</AnchorPane>
