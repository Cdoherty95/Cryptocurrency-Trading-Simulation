   <children>
      <Label layoutX="34.0" layoutY="27.0" text="Are you sure you want to perform this action?" />
      <Button fx:id="continueBtn" layoutX="47.0" layoutY="100.0" mnemonicParsing="false" onAction="#ContinueAction" text="Continue" />
      <Button fx:id="cancelBtn" layoutX="197.0" layoutY="100.0" mnemonicParsing="false" onAction="#CancelAction" text="Cancel" />
   </children>
</AnchorPane>
