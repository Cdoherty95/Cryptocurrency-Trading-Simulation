            <PasswordField fx:id="Password" layoutX="63.0" layoutY="100.0" promptText="Password" />
            <Button fx:id="exitBTN" layoutX="171.0" layoutY="156.0" mnemonicParsing="false" text="Exit" />
            <Label fx:id="Welcome" layoutX="85.0" layoutY="23.0" text="Welcome To Trader" />
            <Label fx:id="errorLable" layoutX="71.0" layoutY="126.0" prefHeight="18.0" prefWidth="130.0" text="hello" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
