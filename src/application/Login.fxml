<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="application.Login">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="275.0">
         <children>
            <TextField fx:id="Username" layoutX="63.0" layoutY="64.0" promptText="Username" />
